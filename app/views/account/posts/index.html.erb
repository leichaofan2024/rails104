<div class="col-md-12">
  <h1>我发表过的文章</h1>
  <table class="table">
    <thead>
      <tr>
        <td>#</td>
        <td>内容</td>
        <td>管理</td>
        <td>更新时间</td>
      </tr>
    </thead>
    <tbody>
      <% @posts.each do |post| %>
      <tr>
        <td>#</td>
        <td><%= post.content %></td>
        <td>
          <%= link_to("Edit", edit_account_post_path(post), class: "btn btn-primary") %>
          <%= link_to("Delete", account_post_path(post), method: "delete" , class: "btn btn-primary") %>
        </td>
        <td><%= post.updated_at%></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
